{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger-ui-init.js",
      "dest": "src/public/swagger-ui-init.js"
    },
    {
      "src": "/swagger-ui.css",
      "dest": "src/public/swagger-ui.css"
    },
    {
      "src": "/api-docs/swagger.json",
      "dest": "src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}
